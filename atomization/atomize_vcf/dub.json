		"Charles Gregory"
	],
	"copyright": "Copyright Â© 2021, Charles Gregory",
	"description": "Convert VCF to line-delimited JSON objects.",
	"license": "MIT",
	"name": "atomize_vcf",
	"dependencies": {
		"asdf": "~>0.7.13",
		"dhtslib": {
			"repository": "git+https://github.com/blachlylab/dhtslib.git",
			"version": "b3fcf27aefff939e111ccb0f18d453c15a51451c"
		},
		"jsonlops": {
			"path": "../../jsonlops"
		}
	},
	"targetPath": "../../bin",
	"configurations": [
		{
			"name": "standard",
			"targetType": "executable"
		},
		{
			"name":"static-alpine",
			"targetType": "executable",
			"dflags-ldc": [
				"-link-defaultlib-shared=false",
				"-static",
				"--linker=gold",
				"-L-lz",
				"-L-lbz2",
				"-L-ldeflate",
				"-L-llzma",
				"-L-lcurl", 
				"-L-lssl", 
				"-L-lssh2", 
				"-L-lcrypto"
			],
			"sourceFiles": ["/usr/local/lib/mimalloc-2.0/mimalloc.o"]
		},
	]
}